{
  "icon": "vcluster.svg",
  "vm": {
    "image": "${DESKTOP_PLUGIN_IMAGE}",
    "exposes": {
      "socket": "vcluster-dd-extension.sock"
    }
  },
  "ui": {
    "dashboard-tab": {
      "title": "vcluster",
      "src": "index.html",
      "root": "ui",
      "backend": {
        "socket": "vcluster-dd-extension.sock"
      }
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "/darwin/kubectl"
          },
          {
            "path": "/darwin/helm"
          },
          {
            "path": "/darwin/vcluster"
          }
        ],
        "windows": [
          {
            "path": "/windows/kubectl.exe"
          },
          {
            "path": "/windows/helm.exe"
          },
          {
            "path": "/windows/vcluster.exe"
          }
        ],
        "linux": [
          {
            "path": "/linux/kubectl"
          },
          {
            "path": "/linux/vcluster"
          },
          {
            "path": "/linux/helm"
          }
        ]
      }
    ]
  }
}